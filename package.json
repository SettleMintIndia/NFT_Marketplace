{
  "name": "@settlemint/solidity-token-erc721-cards",
  "version": "4.0.0",
  "description": "An ERC721 compliant token",
  "scripts": {
    "artengine:build": "yarn hardhat generate-assets",
    "smartcontract:deploy:setup": "yarn hardhat deploy --network ${DEFAULT_NODE} --tags setup",
    "smartcontract:deploy:setup:reset": "yarn hardhat deploy --network ${DEFAULT_NODE} --tags setup --reset",
    "smartcontract:deploy:whitelist": "yarn hardhat deploy --network ${DEFAULT_NODE} --tags whitelist",
    "smartcontract:deploy:presale": "yarn hardhat deploy --network ${DEFAULT_NODE} --tags presale",
    "smartcontract:deploy:publicsale": "yarn hardhat deploy --network ${DEFAULT_NODE} --tags publicsale",
    "smartcontract:deploy:reveal": "yarn hardhat deploy --network ${DEFAULT_NODE} --tags reveal",
    "graph:all": "yarn graph:config && yarn graph:compile && yarn graph:codegen && yarn graph:build && yarn graph:deploy",
    "graph:config": "yarn hardhat graph:config --network ${DEFAULT_NODE}",
    "graph:compile": "yarn graph-compiler --config subgraph.config.json --include node_modules/@openzeppelin/subgraphs/src/datasources node_modules/@settlemint/graph-middleware/src/datasources subgraph/datasources --export-schema --export-subgraph",
    "graph:codegen": "yarn graph codegen generated/solidity-token-erc721-cards.subgraph.yaml",
    "graph:build": "yarn graph build generated/solidity-token-erc721-cards.subgraph.yaml",
    "graph:deploy": "/bin/bash -c 'eval \"$(shdotenv -e ./.secrets/env)\"; if [ \"$MIDDLEWARE\" == \"\" ]; then echo \"You have not launched a graph middleware for this smart contract set, aborting...\"; exit 1; fi; graph create --node ${MIDDLEWARE} ${SCS}; graph deploy --version-label v1.0.$(date +%s) --node ${MIDDLEWARE} --ipfs ${MIDDLEWARE_IPFS} ${SCS} generated/solidity-token-erc721-cards.subgraph.yaml'",
    "lint": "yarn solhint 'contracts/**/*.sol'",
    "compile": "yarn hardhat compile",
    "test": "yarn hardhat test"
  },
  "author": "SettleMint <hello@settlemint.com>",
  "license": "SEE LICENSE IN LICENSE.md",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@0.3.0-beta.13",
    "@nomiclabs/hardhat-etherscan": "3.0.3",
    "@nomiclabs/hardhat-solhint": "2.0.1",
    "@nomiclabs/hardhat-waffle": "2.0.3",
    "@typechain/ethers-v5": "10.0.0",
    "@typechain/hardhat": "6.0.0",
    "@types/chai": "4.3.1",
    "@types/lodash": "4.14.182",
    "@types/mocha": "9.1.1",
    "@types/node": "17.0.21",
    "@types/sharp": "0.30.2",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "chai": "4.3.6",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "ethereum-waffle": "3.4.4",
    "ethers": "5.6.6",
    "hardhat": "2.9.5",
    "hardhat-abi-exporter": "2.9.0",
    "hardhat-deploy": "0.11.5",
    "hardhat-log-remover": "2.0.2",
    "prettier": "2.6.2",
    "prettier-plugin-solidity": "1.0.0-beta.19",
    "solhint": "3.3.7",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "0.7.21",
    "ts-generator": "0.1.1",
    "ts-node": "10.7.0",
    "typechain": "8.0.0",
    "typescript": "4.6.4",
    "yaml": "2.1.0"
  },
  "dependencies": {
    "@amxx/graphprotocol-utils": "1.1.0",
    "@graphprotocol/graph-cli": "^0.30.2",
    "@graphprotocol/graph-ts": "0.27.0",
    "@openzeppelin/contracts": "4.6.0",
    "@openzeppelin/contracts-upgradeable": "4.6.0",
    "@openzeppelin/subgraphs": "0.1.8",
    "@settlemint/graph-middleware": "1.9.18",
    "canvas": "2.9.1",
    "chalk": "4.1.2",
    "form-data": "4.0.0",
    "image-size": "1.0.1",
    "ipfs-http-client": "56.0.3",
    "it-all": "1.0.6",
    "keccak256": "1.0.6",
    "lodash": "4.17.21",
    "merkletreejs": "0.2.31",
    "nft.storage": "6.2.0",
    "sharp": "0.30.4",
    "uint8arrays": "3.0.0"
  },
  "resolutions": {
    "colors": "1.4.0",
    "@web-std/fetch": "4.1.0"
  }
}
